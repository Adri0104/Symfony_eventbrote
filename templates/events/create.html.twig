{% extends 'layouts/base.html.twig' %}

{% block title 'Create new event' %}

{% block body %}
    <div class="row">
        <div class="col-md-6 mx-auto">
            <h1 class="mt-5 h2 font-weight-light">Create new event</h1>
            {{ form_start(formCreate) }}
                {{ form_widget(formCreate) }}
                <p>
